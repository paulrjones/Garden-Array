{
  "name": "Garden-Array",
  "version": "1.0.0",
  "description": "Project 03 - Garden Application",
  "main": "server.js",
  "scripts": {
    "build": "cd client && npm run build",
    "install": "cd client && npm install",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UCI-Bootcamp/Garden-Array.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/UCI-Bootcamp/Garden-Array/issues"
  },
  "homepage": "https://github.com/UCI-Bootcamp/Garden-Array#readme",
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.6",
    "nan": "^2.14.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1"
  }
}
